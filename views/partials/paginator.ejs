<li class=
    <% if(pag_disabled[0] == 'prev') { %>
        'page-item disabled'
    <% } else { %>
        'page-item' %>
    <% } %> >
    <a class="page-link">Previous</a>
</li>
    <% for(var i = 0; i < pag_count; i++) { %>
        
        <% if(pag_active == (i + 1)) { %>
            <li class="page-item active">
                <a class="page-link"><%= (i + 1) %></a>
            </li>
        <% } else { %>
            <li class="page-item">
                <a class="page-link"><%= (i + 1) %></a>
            </li>
        <% } %>
    <% } %>
<li class=
    <% if(pag_disabled[1] == 'next') { %>
        'page-item disabled'
    <% } else { %>
        'page-item' %>
    <% } %> >   
    <a class="page-link">Next</a>
</li>